{
    "name": "diagro/lib_laravel_token",
    "description": "Library for Laravel applications to encode/decode Diagro tokens",
    "type": "library",
    "version": "1.7",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0",
        "firebase/php-jwt": "v5.4.0",
        "ext-openssl" : "*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.10"
    },
    "license": "Beerware",
    "autoload": {
        "psr-4": {
            "Diagro\\Token\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Stijn Leenknegt",
            "email": "stijn@diagro.be"
        }
    ]
}
