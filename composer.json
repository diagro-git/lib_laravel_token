{
    "name": "diagro/lib_laravel_token",
    "description": "Library for Laravel applications to encode/decode Diagro tokens",
    "type": "library",
    "version": "10.0.0",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "firebase/php-jwt": "v6.11.1",
        "ext-openssl" : "*"
    },
    "require-dev": {
        "phpunit/phpunit": "12.2.2"
    },
    "license": "Beerware",
    "autoload": {
        "psr-4": {
            "Diagro\\Token\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Stijn Leenknegt",
            "email": "stijn@diagro.be"
        }
    ]
}
